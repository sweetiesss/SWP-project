                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="#" class="nav-link text-light">Semester</a>
                </li>
            </ul>

            <!-- Right navbar links -->
            <ul class="navbar-nav ml-auto">
                <!-- Navbar Search -->
                <div class="user-panel d-flex ">
                    <div class="image">
                        <img src="../../dist/img/user2-160x160.jpg" class="img-circle elevation-2" alt="User Image" style="width:40px">
                    </div>
                    <div class=" nav-item d-none d-sm-inline-block">
