            <!-- Content Header (Page header) -->
            <section class="content-header">
                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <h1>WELCOME</h1>
                        </div>
                    </div>
                </div><!-- /.container-fluid -->
            </section>

            <form asp-controller="Home" asp-action="Logout">
